{
  "name": "alisp",
  "version": "0.0.1",
  "publisher": "suriing",
  "engines": {
    "vscode": "^1.0.0"
  },
  "description": "alisp syntax highlight",
  "categories": ["Languages", "Snippets"],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/suriing/vscode-alisp"
  },
  "contributes": {
    "languages": [{
      "id": "alisp",
      "aliases": ["alisp", "autolisp", "AutoLisp", "Autolisp"],
      "extensions": [".lsp"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "alisp",
      "scopeName": "source.alisp",
      "path": "./syntaxes/alisp.tmLanguage"
    }],
    "snippets": [{
        "language": "alisp",
        "path": "./snippets/alisp.json"
    }]
  },
  "readmeFilename": "README.md",
  "bugs": {
    "url": "https://github.com/suriing/vscode-alisp/issues"
  }
}
